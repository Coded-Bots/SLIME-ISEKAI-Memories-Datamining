    }, 
    {
        "ColumnName": "balloon_message", 
        "LocalizationTargetId": 101000101, 
        "Text": "Notice: <color=#F7E416>Recruiting</color> has now been unlocked. This is your chance to recruit some powerful new allies."
    }, 
    {
        "ColumnName": "message_group_name", 
        "LocalizationTargetId": 101000102, 
        "Text": "Tutorial Recruit"
    }, 
    {
        "ColumnName": "balloon_message", 
        "LocalizationTargetId": 101000102, 
        "Text": "Normally, recruiting new allies consumes Magicrystals. This time, however, you can do it for free."
    }, 
    {
        "ColumnName": "message_group_name", 
        "LocalizationTargetId": 101000103, 
        "Text": "Tutorial Recruit"
    }, 
    {
        "ColumnName": "balloon_message", 
        "LocalizationTargetId": 101000103, 
        "Text": "You've recruited new characters.\nThey're sure to be valuable allies!"
    }, 
    {
